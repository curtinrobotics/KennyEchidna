<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="kenny_arm">

  <xacro:macro name="kenny_arm_gazebo">

    <gazebo reference="reel">
      <material>Gazebo/Yellow</material>
    </gazebo>

    <gazebo reference="arm">
      <material>Gazebo/Yellow</material>
    </gazebo>

    <gazebo reference="forearm">
      <material>Gazebo/Yellow</material>
    </gazebo>

    <gazebo reference="hand">
      <material>Gazebo/Yellow</material>
    </gazebo>

  <!--  ++++++++++++++++++++++++++++++++++++++++ JOINTS -->

    <gazebo reference="reel_joint">
      <dynamics damping="0.7"/>
      <mu1>0.2</mu1>
      <mu1>0.2</mu1>
    </gazebo>

    <gazebo reference="shoulder">
      <dynamics damping="0.7"/>
      <mu1>0.2</mu1>
      <mu1>0.2</mu1>
    </gazebo>


    <gazebo reference="elbow">
      <dynamics damping="0.7"/>
      <mu1>0.2</mu1>
      <mu1>0.2</mu1>
    </gazebo>

    <gazebo reference="wrist">
      <dynamics damping="0.7"/>
      <mu1>0.2</mu1>
      <mu1>0.2</mu1>
    </gazebo>


  </xacro:macro>

</robot>